{
  "kind": "collectionType",
  "collectionName": "board_members",
  "info": {
    "singularName": "board-member",
    "pluralName": "board-members",
    "displayName": "Board Member"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "Bio": {
      "type": "blocks",
      "required": true
    },
    "position": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::position.position",
      "inversedBy": "board_members"
    }
  }
}
